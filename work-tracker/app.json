{
	"app": {
		"name": "Work tracker",
		"slug": "work-tracker",
		"entry_name": "session",
		"description": "Work session tracker",
		"private": false,
		"icon": "üë®üèª‚Äçüíª",
		"color": "#4de1ff",
		"storageType": "",
		"storage": "",
		"charts_placement": "",
		"entries_name": "sessions"
	},
	"temporal": {
		"times": "yes",
		"ranges": "yes"
	},
	"entry": [
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"expression": "weekday(date, 'shortname')"
			},
			"name": "weekday"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"default": "[$today]",
				"formatting": [
					"b"
				]
			},
			"name": "date"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " at ",
				"type": "time",
				"default": "[time($startup, 'hours')]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(date, time)"
			},
			"name": "date_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " to ",
				"type": "time",
				"default": "",
				"visible": "[time]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "end_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"prefix": " on ",
				"visible": "[if(time and end_time, if(end_date = date, 'edit', true), false)]",
				"default": "[if(end_time, if(end_time < time, date(date + 1 * days()), date))]",
				"min_date": "[date]"
			},
			"name": "end_date"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(end_date, end_time)"
			},
			"name": "end_date_time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "end_date_time - date_time"
			},
			"name": "duration"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"visible": "[end_date and (time and end_time)]",
				"prefix": "(",
				"suffix": ")",
				"expression": "duration(duration, 2)"
			},
			"name": "duration_readable"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "text",
				"appearance": "line",
				"multiline": true
			},
			"name": "notes"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "options",
				"default": "Personal",
				"allow_multiple": "",
				"multiline": false,
				"visible": "",
				"options": "Personal, Quest, Breeze, Synergy"
			},
			"name": "project"
		},
		{
			"type": "expression",
			"scope": "entry",
			"settings": {
				"prefix": "‚Ç¨",
				"expression": "if(project = \"Personal\", 0, hours(end_date_time - date_time) * 50)"
			},
			"name": "Earnings"
		}
	],
	"root": [
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"visible": "edit",
				"label": "‚ûï Add session",
				"target": "add(entry)",
				"target_preset": "new",
				"appearance": "",
				"set_pairs": [],
				"filter_pairs": []
			}
		},
		{
			"type": "expression",
			"scope": "root",
			"settings": {
				"expression": "sum(Earnings)",
				"prefix": "Totalone: ",
				"suffix": "‚Ç¨"
			}
		}
	],
	"charts": [
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"aggregate": "sum",
				"aggregate_of": "Earnings",
				"group_by": "project"
			}
		},
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"type": "table",
				"aggregate_of": "Earnings",
				"group_by": "notes",
				"aggregate": "average"
			}
		}
	]
}